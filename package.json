{
  "name": "@stylelint-types/stylelint-order",
  "type": "module",
  "version": "6.0.4",
  "packageManager": "pnpm@10.8.1",
  "description": "TypeScript definitions for stylelint-define-config",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "license": "MIT",
  "homepage": "https://github.com/stylelint-types/stylelint-order#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stylelint-types/stylelint-order.git"
  },
  "bugs": "https://github.com/stylelint-types/stylelint-order/issues",
  "keywords": [
    "config",
    "scss",
    "configuration",
    "define-config",
    "stylelint-config",
    "stylelint",
    "stylelint-order",
    "stylelintconfig",
    "typed",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts"
    },
    "./types": {
      "types": "./lib/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp package.json --execute=\"pnpm release:changelog\" --commit --all --push --tag && pnpm publish --access public"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0",
    "stylelint-define-config": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "stylelint": {
      "optional": true
    }
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^1.29.0",
    "bumpp": "^10.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.25.0",
    "stylelint": "^16.18.0",
    "stylelint-define-config": "^16.18.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3"
  }
}
